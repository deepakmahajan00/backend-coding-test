{
  "name": "backend-coding-test",
  "version": "1.0.0",
  "description": "A coding assessment for Xendit backend interviews",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha tests",
    "start": "node index.js ts-node ./src/index.ts",
    "lint": "eslint ./src --fix",
    "tsc": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test"
    }
  },
  "nyc": {
    "check-coverage": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
  },
  "keywords": [
    "Xendit"
  ],
  "author": "Bo",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.16.4",
    "sqlite3": "^4.0.6",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "eslint": "^8.10.0",
    "mocha": "^6.1.4",
    "nyc": "^15.1.0",
    "supertest": "^4.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
